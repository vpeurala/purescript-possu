{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5
  },
  "env": {
    "node": true
  },
  "rules": {
    "indent": [
      "error",
      4
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "off"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      "off"
    ]
  }
}