{
  "name": "purescript-possu",
  "version": "0.0.1",
  "description": "An alternative PureScript binding library to npm library node-postgres (also known as pg).",
  "keywords": [
    "purescript",
    "postgres",
    "postgresql",
    "node",
    "node-postgres",
    "pg",
    "possu"
  ],
  "license": "MIT",
  "homepage": "https://github.com/vpeurala/purescript-possu",
  "bugs": "https://github.com/vpeurala/purescript-possu/issues",
  "repository": "git@github.com:vpeurala/purescript-possu.git",
  "author": "Ville Peurala <ville.peurala@gmail.com> (https://villepeurala.fi)",
  "files": [
    "src"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js test/**/*.js",
    "refresh": "rm -rf yarn.lock bower_components node_modules output .purs-repl .pulp-cache .psci_modules && yarn install --flat --force --non-interactive && bower install --force",
    "test": "NODE_ENV=development pulp test"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "pg": "^7.3.0"
  },
  "devDependencies": {
    "eslint": "^4.9.0"
  },
  "flat": true,
  "private": false,
  "resolutions": {
    "semver": "5.4.1",
    "chalk": "2.2.0",
    "strip-ansi": "4.0.0",
    "ansi-styles": "3.2.0",
    "supports-color": "4.5.0",
    "ansi-regex": "3.0.0",
    "acorn": "5.1.2"
  }
}
